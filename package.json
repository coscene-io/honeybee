{
  "name": "honeybee",
  "version": "0.2.1-rc1",
  "license": "MPL-2.0 AND Proprietary",
  "private": true,
  "productName": "coStudio",
  "description": "coScene's robot visualization platform",
  "productDescription": "coScene Studio",
  "packageManager": "yarn@3.6.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/coscene-io/honeybee.git"
  },
  "author": {
    "name": "coScene Technologies",
    "email": "hi@coscene.io"
  },
  "homepage": "https://www.coscene.cn/",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "clean": "tsc --build --clean packages/**/tsconfig.json && rimraf .webpack */.webpack dist storybook-screenshots storybook-static",
    "build:packages": "tsc --build --verbose packages/**/tsconfig.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "web:build:dev": "rspack build --mode development --config web/rspack.config.ts",
    "web:build:prod": "rspack build --mode production --config web/rspack.config.ts",
    "web:serve": "rspack serve --env WEBPACK_SERVE=true --mode development --config web/rspack.config.ts",
    "desktop:build:dev": "rspack build --mode development --config desktop/rspack.config.ts",
    "desktop:build:prod": "rspack build --mode production --config desktop/rspack.config.ts",
    "desktop:serve": "rspack serve --env WEBPACK_SERVE=true --mode development --config desktop/rspack.config.ts",
    "desktop:start": "yarn workspace desktop electron .webpack",
    "benchmark:serve": "rspack serve --mode development --config benchmark/rspack.config.ts",
    "benchmark:build:prod": "rspack --mode production --config benchmark/rspack.config.ts",
    "license-check": "ts-node ci/license-check.ts",
    "lint": "TIMING=1 eslint --report-unused-disable-directives --fix .",
    "lint:ci": "TIMING=1 eslint --report-unused-disable-directives --config eslint.config.ci.cjs .",
    "lint:unused-exports": "ts-node ./ci/lint-unused-exports.ts",
    "lint:dependencies": "ts-node ./ci/lint-dependencies.ts .",
    "lint:inspect": "eslint --inspect-config",
    "test": "jest",
    "test:debug": "cross-env NODE_OPTIONS='--inspect-brk' jest",
    "test:watch": "jest --watch",
    "test:web-integration": "jest --forceExit --projects  web/integration-test",
    "test:web-integration:local": "BASE_URL=\"http://localhost:8080/\" jest --forceExit --projects  web/integration-test",
    "package:win": "yarn package --win",
    "package:darwin": "yarn package --mac",
    "package:linux": "yarn package --linux",
    "package": "cross-env NODE_OPTIONS='-r ts-node/register' electron-builder build --config desktop/electronBuilderConfig.js --publish never",
    "package:e2e": "yarn && yarn clean && yarn desktop:build:prod && yarn package",
    "release:bump-nightly-version": "node -r ts-node/register ./ci/bump-nightly-version.ts",
    "storybook": "storybook dev --port 9009 --config-dir .storybook",
    "storybook:build": "storybook build --config-dir .storybook --webpack-stats-json",
    "version:all": "yarn workspaces foreach version"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/@types/*",
      "web",
      "benchmark",
      "desktop"
    ]
  },
  "resolutions": {
    "@types/node": "20.10.0",
    "@types/react": "18.3.27",
    "react-use": "patch:react-use@17.4.0#./patches/react-use.patch",
    "react-dnd": "patch:react-dnd@npm:16.0.1#./patches/react-dnd.patch",
    "react-is": "18.3.1"
  },
  "devDependencies": {
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/tool-cache": "2.0.1",
    "@babel/core": "7.29.0",
    "@babel/plugin-proposal-explicit-resource-management": "7.27.4",
    "@babel/preset-env": "7.29.0",
    "@babel/preset-react": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@foxglove/eslint-plugin": "2.0.0",
    "@foxglove/eslint-plugin-studio": "workspace:*",
    "@foxglove/tsconfig": "2.0.0",
    "@rspack/cli": "1.6.7",
    "@rspack/core": "1.6.7",
    "@storybook/addon-actions": "8.2.8",
    "@storybook/addon-essentials": "8.2.8",
    "@storybook/addon-interactions": "8.2.8",
    "@storybook/react": "8.2.8",
    "@storybook/react-webpack5": "8.2.8",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.10.0",
    "@types/case-sensitive-paths-webpack-plugin": "2.1.9",
    "@types/jest": "29.5.14",
    "@types/license-checker": "^25.0.6",
    "@types/semver": "^7.5.6",
    "babel-plugin-transform-import-meta": "2.2.1",
    "cross-env": "7.0.3",
    "depcheck": "1.4.7",
    "electron-builder": "26.0.11",
    "eslint": "9.16.0",
    "eslint-import-resolver-webpack": "0.13.8",
    "eslint-plugin-file-progress": "3.0.1",
    "eslint-plugin-storybook": "0.11.1",
    "eslint-plugin-tss-unused-classes": "1.0.2",
    "glob": "13.0.0",
    "globals": "15.13.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "license-checker": "25.0.1",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "semver": "7.5.4",
    "storybook": "8.2.8",
    "ts-node": "10.9.2",
    "ts-unused-exports": "10.0.1",
    "tslib": "2.8.1",
    "typescript": "5.3.3",
    "webpack": "5.97.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.0",
    "webpack-hot-middleware": "2.26.0"
  }
}
